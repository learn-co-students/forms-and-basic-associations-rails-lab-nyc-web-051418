<h1>Create a Song</h1>

<%= form_for @song do |f| %>
<p>Song Title</p>
<%= f.text_field :title %>
<p>Artist Name</p>
<%= f.text_field :artist_name %>
<p>Genre</p>
<%= f.collection_select :genre_id, Genre.all, :id, :name %>
<br>
<p>Note 1: <input name='song[note_contents][]' id='song_notes_1'>
Note 2: <input name='song[note_contents][]' id='song_notes_2'>
Note 3: <input name='song[note_contents][]' id='song_notes_3'></p>
<%= f.submit %>
<% end %>
